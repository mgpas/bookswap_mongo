{
  "expo": {
    "name": "BS",
    "slug": "BS",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#C0A48C"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#C0A48C"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
    [
      "expo-font",
      {
        "fonts": ["node_modules/@expo-google-fonts/libre-baskerville/LibreBaskerville_400Regular.ttf"]
      }
    ]
  ]
  }
}
